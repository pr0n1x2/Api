    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "npm",
            "protocol": "inspector",
            "cwd": "${workspaceRoot}/server",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run",
                "devstart"
            ],
            "restart": true,
            "console": "integratedTerminal",
            "skipFiles": [
                "node_modules/**"
            ],
            "port": 9898
        }
    ]
}